<h1>New deliverable</h1>

<%= error_messages_for :deliverable %>

<% form_for(@deliverable) do |f| %>
  <div>
    <span class='key'>Name</span><br />
    <%= f.text_field :name %>
  </div>

  <div>
    <span class='key'>Description</span><br />
    <%= f.text_area :description %>
  </div>
	
<div>
	tags for this deliverable: <%= f.text_field :tag_list %>
</div>

  <div>
    <%= f.submit "Create" %>
  </div>
<% end %>

<%= link_to 'Back', deliverables_path %>
