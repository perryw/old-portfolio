<h1>New project</h1>

<%= error_messages_for :project %>

<% form_for(@project) do |f| %>
  <div>
    <span class='key'>Name</span><br />
    <%= f.text_field :name %>
  </div>

  <div>
    <span class='key'>Description</span><br />
    <%= f.text_area :description %>
  </div>

  <div>
    <%= f.submit "Create" %>
  </div>
<% end %>

<%= link_to 'Back', projects_path %>
