<h1>New course</h1>

<%= error_messages_for :course %>

<% form_for(@course) do |f| %>
  <p>
    <b>Dept</b><br />
    <%= f.text_field :dept %>
  </p>

  <p>
    <b>Courseno</b><br />
    <%= f.text_field :courseno %>
  </p>
  <p>
    <b>Coursename</b><br />
    <%= f.text_field :coursename %>
  </p>
  <p>
    <b>Description</b><br />
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', courses_path %>
